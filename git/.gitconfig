[alias]
	a = add
	c = commit
	co = checkout
	d = diff
	dc = diff --cached
	f = fetch
	fix = commit --fixup
	fp = fetch --prune
	l = !git log --graph --decorate --pretty=oneline --abbrev-commit --all || true
	lol = !git log --graph --decorate --pretty=oneline --abbrev-commit --all || true
	ls-ignored = ls-files -o -i --exclude-standard
	p = push
	s = status --short
	st = status
[color]
	ui = auto
[core]
	excludesFile = ~/.config/git/ignore
	hooksPath = ~/.config/git/hooks
[init]
	defaultBranch = master
[pull]
	ff = only
[user]
	name = Jaakko Kantoj√§rvi
	email = jaakko@n-1.fi

[include]
	path = ~/.gitconfig.local

